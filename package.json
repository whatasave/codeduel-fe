{
	"name": "codeduel-fe",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "vite dev --host 127.0.0.1",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"docker:build": "docker build -t xedom/codeduel-fe .",
		"docker:push": "docker push xedom/codeduel-fe",
		"docker:up": "docker run -p 8000:80 xedom/codeduel-fe",
		"docker:down": "docker stop xedom/codeduel-fe",
		"docker:clean": "docker system prune -f",
		"docker:restart": "yarn docker:down && yarn docker:up"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.2.0",
		"@sveltejs/adapter-node": "^5.0.1",
		"@sveltejs/kit": "^2.5.5",
		"@sveltejs/vite-plugin-svelte": "^3.1.0",
		"@types/dompurify": "^3.0.5",
		"@types/eslint": "^8.56.0",
		"@typescript-eslint/eslint-plugin": "^7.0.0",
		"@typescript-eslint/parser": "^7.0.0",
		"autoprefixer": "^10.4.19",
		"class-variance-authority": "^0.7.0",
		"clsx": "^2.1.1",
		"dayjs": "^1.11.11",
		"dompurify": "^3.1.2",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.36.0-next.4",
		"marked": "^12.0.2",
		"monaco-editor": "^0.48.0",
		"postcss": "^8.4.38",
		"prettier": "^3.2.5",
		"prettier-plugin-svelte": "^3.1.2",
		"prettier-plugin-tailwindcss": "^0.5.14",
		"svelte": "^5.0.0-next.123",
		"svelte-check": "^3.7.1",
		"tailwindcss": "^3.4.3",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.13"
	},
	"packageManager": "pnpm@9.0.6+sha256.0624e30eff866cdeb363b15061bdb7fd9425b17bc1bb42c22f5f4efdea21f6b3"
}
