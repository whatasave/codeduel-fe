{
	"name": "frontend",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"packageManager": "yarn@4.1.1",
	"compilerOptions": {
		"lib": [
			"es2015"
		]
	},
	"scripts": {
		"dev": "vite dev --host 127.0.0.1",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"docker:build": "docker build -t xedom/codeduel-fe .",
		"docker:push": "docker push xedom/codeduel-fe",
		"docker:up": "docker run -p 8000:80 xedom/codeduel-fe",
		"docker:down": "docker stop xedom/codeduel-fe",
		"docker:clean": "docker system prune -f",
		"docker:restart": "yarn docker:down && yarn docker:up"
	},
	"devDependencies": {
		"@eslint/js": "^9.1.1",
		"@sveltejs/adapter-auto": "^3.2.0",
		"@sveltejs/adapter-node": "^5.0.1",
		"@sveltejs/kit": "^2.5.5",
		"@types/dompurify": "^3.0.5",
		"autoprefixer": "^10.4.16",
		"class-variance-authority": "^0.7.0",
		"clsx": "^2.1.0",
		"dayjs": "^1.11.10",
		"dompurify": "^3.0.8",
		"eslint": "^9.1.1",
		"globals": "^15.1.0",
		"marked": "^11.1.1",
		"monaco-editor": "^0.45.0",
		"postcss": "^8.4.32",
		"prettier": "^3.2.5",
		"prettier-plugin-svelte": "^3.1.2",
		"prettier-plugin-tailwindcss": "^0.5.13",
		"svelte": "^5.0.0-next.123",
		"svelte-check": "^3.6.0",
		"tailwindcss": "^3.4.0",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"typescript-eslint": "^7.8.0",
		"vite": "^5.0.13"
	}
}
